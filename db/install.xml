<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/grading_report/db" VERSION="20130407" COMMENT="XMLDB file for local message plugin"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="block_grading_report_units" COMMENT="Enabled list of quizid">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="15" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="course" TYPE="char" LENGTH="45" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="course_code" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="unit_code" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="activities" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>