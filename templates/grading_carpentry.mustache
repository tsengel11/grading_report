{{!
    @template block/student_dashboard
    Example context (json):
    {
    }
}}
<h1>Grading Report</h1>
<p>{{texttodisplay}}</p>

<div class="dropdown show">
  <input type="button" class="btn btn-primary" value="Back" onclick="location.href='https://moodle.libertycollege.edu.au/'">
  <a class="custom-select" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    Select Groups
  </a>

    <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
        {{#cohorts}}
      {{{drop_downitem}}}
        {{/cohorts}}
    </div>
    <th>
    <div><b>Group Name : </b>{{groupname}}</div>
    <div><b>Total number of students : </b>{{studentnumber}}</div>
     
</div>

  <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col" style="width: 100px">Student Name</th>
      <th scope="col">Student ID</th>
      <th scope="col">Student Email</th>
      <th scope="col">CPCCOHS2001A Practical</th>
      <th scope="col">CPCCCM1012A Practical</th>
      <th scope="col">CPCCCM1013A Practical(SWMS)</th>
      <th scope="col">CPCCCM1013A Practical(Photos)</th>
      <th scope="col">CPCCCM1014A Practical</th>
      <th scope="col">CPCCCM1015A Practical</th>
      <th scope="col">CPCCCM2001A Practical</th>
      <th scope="col">CPCCCA2011A Practical(SWMS)</th>
      <th scope="col">CPCCCA2011A Practical(Photos)</th>
      <th scope="col">CPCCCA2002B Practical(SWMS)</th>
      <th scope="col">CPCCCA2002B Practical(Photos)</th>
      <th scope="col">CPCCCA3002A Practical(SWMS)</th>
      <th scope="col">CPCCCA3002A Practical(Photos)</th>
    </tr>
  </thead>
  <tbody>
  {{#users}}
    <tr>
    <td>{{{count}}}</td>
    <td> {{{userlink}}}</td>
    <td>{{{id}}}</td>
    <td>{{{email}}}</td>
    <td>{{{cpccohs2001a_practical}}}</td>
    <td>{{{cpcccm1012a_practical}}}</td>
    <td>{{{cpcccm1013a_practical_swms}}}</td>
    <td>{{{cpcccm1013a_practical_photo}}}</td>
    <td>{{{cpcccm1014a_practical}}}</td>
    <td>{{{cpcccm1015a_practical}}}</td>
    <td>{{{cpcccm2001a_practical}}}</td>
    <td>{{{cpccca2011a_practical_swms}}}</td>
    <td>{{{cpccca2011a_practical_photo}}}</td>
    <td>{{{cpccca2002b_practical_swms}}}</td>
    <td>{{{cpccca2002b_practical_photo}}}</td>
    <td>{{{cpccca3002a_practical_swms}}}</td>
    <td>{{{cpccca3002a_practical_photo}}}</td>
    </tr>
    {{/users}}
  </tbody>
</table>
    

