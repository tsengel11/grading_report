<h1>Grading Report</h1>
<p>{{texttodisplay}}</p>

<div class="dropdown show">
  <input type="button" class="btn btn-primary" value="Back" onclick="location.href='https://moodle.libertycollege.edu.au/'">
  <a class="custom-select" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    Select Groups
  </a>
    <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
        {{#cohorts}}
        {{{drop_downitem}}}
        {{/cohorts}}
    </div>
    <th>
    <div><b>Group Name : </b>{{groupname}}</div>
    <div><b>Total number of students : </b>{{studentnumber}}</div>
     
</div>

  <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Student Name</th>
      <th scope="col">Student Email</th>
      {{#units}}
      <th scope="col"><small>{{{assigment_link}}}</small></th>
      {{/units}}
       <th scope="col">Student Name</th>

    </tr>
  </thead>
  <tbody>
  {{#users}}
    <tr>
    <td>{{{count}}}</td>
    <td>{{{userlink}}}</td>
    <td>{{{email}}}</td>
    <td>{{{3692}}}</td>
    <td>{{{3695}}}</td>
    <td>{{{3691}}}</td>
    <td>{{{3694}}}</td>
    <td>{{{3686}}}</td>
    <td>{{{3288}}}</td>
    <td>{{{3689}}}</td>
    <td>{{{3690}}}</td>
    <td>{{{3447}}}</td>
    <td>{{{3583}}}</td>
    <td>{{{3585}}}</td>
    <td>{{{3768}}}</td>
    <td>{{{userlink}}}</td>
    </tr>
    {{/users}}
  </tbody>
</table>
    

